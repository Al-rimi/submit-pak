{
    "name": "al-rimi/submit",
    "description": "A fully customizable submission system for your students",
    "type": "library",
    "require": {
        "php": ">=8.0",
        "laravel/framework": "^10.0",
        "illuminate/bus": "^10.0",
        "illuminate/contracts": "^10.0",
        "illuminate/mail": "^10.0",
        "illuminate/queue": "^10.0",
        "illuminate/support": "^10.0",
        "illuminate/http": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "AlRimi\\Submit\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlRimi\\Submit\\Providers\\SubmitServiceProvider"
            ],
            "aliases": {
                "Submit": "AlRimi\\Submit\\Facades\\Submit"
            }
        }
    },
    "license": "MIT",
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
